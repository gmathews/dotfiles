[push]
    default = simple
[core]
	excludesfile = /Users/george/.gitignore_global
    pager = diff-so-fancy | less --tabs=4 -RFX
    editor = vim
[merge]
	tool = vimdiff
	conflictstyle = diff3
[mergetool]
	prompt = false
[commit]
	template = /Users/george/.stCommitMsg
[filter "lfs"]
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
